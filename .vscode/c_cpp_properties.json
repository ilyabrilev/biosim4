{
    "configurations": [
        {
            "name": "Linux",
            "browse": {
                "path": [
                    "${workspaceFolder}",
                    "${workspaceFolder}/src/*",
                    "${workspaceFolder}/src/nested/*"
                ],
                "limitSymbolsToIncludedHeaders": true
            },
            "includePath": [
                "${default}",
                "/usr/lib/x86_64-linux-gnu",
                "/usr/include",
                "/usr/include/opencv4"
            ],
            "defines": [],
            "cStandard": "c17",
            "cppStandard": "gnu++17",
            "intelliSenseMode": "linux-gcc-x64"
        }
    ],
    "version": 4
}